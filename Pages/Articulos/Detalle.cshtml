@page
@model CRUDCatalogoWeb.Pages.Articulos.DetalleModel
@{

    <table class="table">
        <thead>
            <tr>
                <th scope="col">Marca</th>
                <th scope="col">Modelo</th>
                <th scope="col">Precio</th>
                <th scope="col">Stock</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>@Model.a?.marca</td>
                <td>@Model.a?.modelo</td>
                <td>@Model.a?.precio</td>
                <td>@Model.a?.stock</td>
            </tr>
        </tbody>
    </table>

    <a href="/"
       class="btn btn-light">
        Volver
    </a>
}
